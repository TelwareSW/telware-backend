{
  "name": "telware-backend",
  "version": "1.0.0",
  "description": "Backend Repo for TelWare Project",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "npx nodemon -r tsconfig-paths/register src/index.ts",
    "seed": "npx ts-node -r tsconfig-paths/register src/database/seed/seed.ts --import",
    "drop": "npx ts-node -r tsconfig-paths/register src/database/seed/seed.ts --delete",
    "migrate:create": "npx migrate-mongo create",
    "migrate:up": "npx migrate-mongo up",
    "migrate:down": "npx migrate-mongo down",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "format": "npx prettier --write ."
  },
  "author": "TelWare",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.9",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.11.0",
    "migrate-mongo": "^11.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.16",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.7.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.15",
    "redis": "^4.7.0",
    "request": "^2.88.2",
    "telware-backend": "file:",
    "validator": "^13.12.0"
  }
}
